{
  "name": "lead-ingestor",
  "version": "1.0.0",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "ts-node-dev --respawn --transpile-only ts/index.ts",
    "start": "node dist/index.js",
    "test": "node scripts/testPowerplay.js",
    "export:events": "node scripts/export-events.js",
    "export:mongo": "node scripts/exportEvents.js",
    "health": "node scripts/health.mjs",
    "api:map": "node scripts/mapPowerPlayAPI.mjs",
    "cookies:save": "node scripts/cookieSaver.js",
    "opps:fetch": "node scripts/fetch-opps-once.js",
    "opps:test": "node scripts/test-opportunities.mjs",
    "dev:pp": "node --enable-source-maps scripts/dev-powerplay.js",
    "probe:pp": "node scripts/probe-powerplay.js",
    "cookies:refresh": "node scripts/refreshAllCookies.js",
    "render:start": "npx playwright install chromium && node src/index.js"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "ts-node-dev": "^2.0.0",
    "vitest": "^2.1.4",
    "@types/node": "^20.12.12",
    "eslint": "^9.12.0"
  },
  "dependencies": {
    "@microsoft/signalr": "^8.0.6",
    "axios": "^1.7.7",
    "@sendgrid/mail": "^8.1.0",
    "dotenv": "^16.6.1",
    "mongoose": "^8.7.0",
    "playwright": "^1.48.0",
    "express": "^4.19.2",
    "mongodb": "^6.9.0"
  }
}
